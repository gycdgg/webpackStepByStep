{
    "extends": [".eslintrc.json"],
    "rules": {
        "no-multiple-empty-lines": ["warn", {"max": 1}],
        "indent": ["warn", 4, {"SwitchCase": 1}],
        "no-console": ["warn", {"allow": ["warn"]}],
        "no-debugger": "warn",
        "no-alert": "warn",
        "no-eval": "warn"
    }
}